package com.sist.vo;

public class MemberBean {
	// bean은 객체를 의미한다
	// usebean은 기본생성자가 제공된다
	// usebean은 기본생성자가 제공되므로 매개변수가 있는 생성자를 만들면 오류가 발생
	// 해결하기 위해서는 기본생성자 포함 생성자를 만들어야한다
	private String id;
	private String pwd;
	private String name;
	private String birth;
	private String email;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		System.out.println("id 동작");
		this.id = id;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		System.out.println("비밀번호 동작");
		this.pwd = pwd;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		System.out.println("이름 동작");
		this.name = name;
	}
	public String getBirthday() {
		return birth;
	}
	public void setBirthday(String birth) {
		System.out.println("생일 동작");
		this.birth = birth;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		System.out.println("이메일 동작");
		this.email = email;
	}
	public MemberBean(String id, String pwd, String name, String birth, String email) {
		super();
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.birth = birth;
		this.email = email;
	}
	public MemberBean() {
		super();
	}
}
