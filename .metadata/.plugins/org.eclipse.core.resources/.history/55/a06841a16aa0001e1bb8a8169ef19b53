<%
	request.setCharacterEncoding("UTF-8");
	String id = request.getParameter("id");
	String pwd = request.getParameter("pwd");
	// 세션에 상태유지를 위한 값을 설정
	session.setAttribute("idKey", id);
	
	// 세션의 유효기간을 초단위로 설정
	// 기본값은 30분
	session.setMaxInactiveInterval(60 * 5);
%>
